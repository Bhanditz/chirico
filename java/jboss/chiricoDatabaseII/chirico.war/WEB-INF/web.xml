<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5"
         xmlns="http://java.sun.com/xml/ns/javaee" 
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	<servlet>
		<servlet-name>Hello Servlet</servlet-name>
		<servlet-class>com.chirico.HelloWorldServlet</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Hello Servlet</servlet-name>
		<url-pattern>/sayhello</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>Insert Servlet</servlet-name>
		<servlet-class>com.chirico.InsertHours</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>Insert Servlet</servlet-name>
		<url-pattern>/insert</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>itemsDP2</servlet-name>
		<servlet-class>com.chirico.itemsDP2</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>itemsDP2</servlet-name>
		<url-pattern>/flash/examples2/itemsDP2</url-pattern>
	</servlet-mapping>

	<servlet>
		<servlet-name>statsT</servlet-name>
		<servlet-class>com.chirico.statsT</servlet-class>
	</servlet>

	<servlet-mapping>
		<servlet-name>statsT</servlet-name>
		<url-pattern>/flash/examples2/statsT</url-pattern>
	</servlet-mapping>



</web-app>
